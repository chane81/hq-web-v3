{
  "name": "my-app",
  "private": true,
  "scripts": {
    "docker-build": "docker build --build-arg PROFILE=dev -t hq-admin-web -f ./Dockerfile .",
    "docker-run": "docker run -d --name hq-admin-web -p 8080:8080 hq-admin-web:latest",
    "ecr-push": "sh ./docker/ecrPush.sh",
    "tw-watch": "cross-env TAILWIND_MODE=watch tailwind -i src/styles/tailwind.css -o src/styles/build.css --watch",
    "tw-build": "cross-env NODE_ENV=production tailwind -i src/styles/tailwind.css -o src/styles/build.css",
    "dev": "concurrently \"yarn:tw-watch\" \"yarn:next dev --port 8080\"",
    "build:prod": "yarn ts-check && yarn tw-build && next build",
    "build": "cross-env NODE_ENV=production yarn lint && yarn ts-check && yarn tw-build && next build",
    "start": "next start --port 8080",
    "ts-check": "tsc --pretty --project tsconfig.json --noEmit",
    "format": "prettier --write .",
    "lint": "next lint"
  },
  "dependencies": {
    "@ag-grid-community/all-modules": "^26.2.0",
    "@ag-grid-community/client-side-row-model": "^26.2.0",
    "@ag-grid-community/core": "^26.2.0",
    "@ag-grid-community/react": "^26.2.0",
    "@date-io/dayjs": "^2.11.0",
    "@hookform/resolvers": "^2.8.5",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.57",
    "@mui/material": "^5.2.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.7",
    "mobx-devtools-mst": "^0.9.30",
    "mobx-react": "^7.2.1",
    "mobx-react-devtools": "^6.1.1",
    "mobx-react-lite": "^3.2.2",
    "mobx-state-tree": "^5.0.5",
    "mobx-utils": "^6.0.4",
    "next": "12.0.4",
    "next-compose-plugins": "^2.2.1",
    "process": "^0.11.10",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "7.15.4",
    "react-icons": "^4.3.1",
    "react-query": "^3.33.6",
    "react-spring": "^9.3.2",
    "react-toastify": "^8.1.0",
    "sass": "^1.43.5",
    "tailwind-override": "^0.6.0",
    "tailwindcss-filters": "^3.0.0",
    "twin.macro": "^2.8.1",
    "universal-cookie": "^4.0.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.4",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/cache": "^11.6.0",
    "@emotion/css": "^11.5.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/component": "^5.15.0",
    "@types/lodash": "^4.14.177",
    "@types/node": "16.11.10",
    "@types/react": "17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^2.2.4",
